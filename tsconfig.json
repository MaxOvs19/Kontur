    "jsx": "react",
    "target": "es2016",
    "rootDir": "src",
    "outDir": "build",
    "module": "commonjs",

    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
